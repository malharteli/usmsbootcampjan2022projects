<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<!-- Create Bean Instance -->
	<bean id="customer1" class="com.virtusa.springcorelab.models.Customer">
	<!-- DI -->
	<constructor-arg index="0" value="32467"></constructor-arg>
	<constructor-arg index="1" value="Parameswari"></constructor-arg>
	</bean>
	<bean id="customer2" class="com.virtusa.springcorelab.models.Customer">
	<!-- DI -->
	<property name="customerId" value="5979879"></property>
	<property name="name" value="Vignesh"></property>
	</bean>
	</beans>